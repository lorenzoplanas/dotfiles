set t_Co=256
set go-=T
set background=light
let g:solarized_termcolors=256
syntax enable
colorscheme solarized

set number
set columns=83
set tabpagemax=100
set hlsearch
set tabstop=2
set smarttab
set shiftwidth=2
set autoindent
set expandtab
set backspace=start,indent

let mapleader = ","
nmap <leader>f :FufFile<cr>
